{
    "name": "jiny/site-board",
    "description": "jinySite Board for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "jiny/posts": "^0.0.3"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Site\\Board\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Site\\Board\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Site\\Board\\JinySiteBoardServiceProvider"
            ]
        }
    }

}
